      }
    },
    {
      "document_type": "md",
      "chunking": {
        "strategy": "layout",
        "size": 500,
        "overlap": 100
      },
      "loading": {
        "strategy": "web"
      }
    },
    {
      "document_type": "html",
      "chunking": {
        "strategy": "layout",
        "size": 500,
        "overlap": 100
      },
      "loading": {
        "strategy": "web"
      }
    },
    {
      "document_type": "docx",
